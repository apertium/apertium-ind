<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <mode name="ind-morph" install="yes">
    <pipeline>
      <program name="hfst-proc">
        <file name="ind.automorf.hfst"/>
      </program>
    </pipeline>
  </mode>

  <mode name="ind-gener" install="yes">
    <pipeline>
      <program name="hfst-proc -g">
        <file name="ind.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

</modes>

