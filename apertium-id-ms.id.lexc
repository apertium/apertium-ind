!!! -- Morphotactic Rules --
!!! 
!!! 
!!! ----------------------------------------------------------------

!! ROOT node
!! Includes the Pronoun Proclitics
LEXICON Root
aku%<prn%>%<pro%>%<p1%>%<sg%>%+@P.FLAG.verbinfl@:ku000000@P.FLAG.verbinfl@                   VerbInflection ;
aku%<prn%>%<pro%>%<p1%>%<sg%>%<cap%>%+@P.FLAG.verbinfl@:Ku0000000@P.FLAG.verbinfl@           VerbInflection ;
kamu%<prn%>%<pro%>%<p2%>%<sg%>%+@P.FLAG.verbinfl@:kau000000@P.FLAG.verbinfl@                 VerbInflection ;
kamu%<prn%>%<pro%>%<p2%>%<sg%>%<cap%>%+@P.FLAG.verbinfl@:Kau0000000@P.FLAG.verbinfl@         VerbInflection ;
@P.CAP.on@                                                                                   MainWord ;
                                                                                             MainWord ;
!! MAIN WORD branch
!! Includes:
!! - Verb and Numeral Inflection
!! - Adjective and Noun Derivation
!! - Others
LEXICON MainWord
@P.FLAG.numinfl@                                   NumeralInflection ;
@P.FLAG.deradj@                                    DerivingAdjective ;
@P.FLAG.dernoun@                                   DerivingNoun-Singular ;
@P.FLAG.dernoun@                                   DerivingNoun-Plural ;
@P.FLAG.verbinfl@                                  VerbInflection ;
                                                   Pronoun ;
                                                   Subordinating ;
                                                   Coordinating ;
                                                   Determiner ;
                                                   Adverb ;
                                                   Question ;
                                                   Modal ;
                                                   Interjection ;
                                                   Particle ;
                                                   Preposition ;
                                                   Exception ;



!! -- Section: PREFIX

!! Numeral Inflections
LEXICON NumeralInflection
@P.MARK.bare@                                                               Numeral ;  ! eg. "tujuh" [seven]
00@P.MARK.ke@:ke@P.MARK.ke@                                                 Numeral ;  ! eg. "ke- + tujuh" [seventh]
00@P.MARK.ke@@P.REDUP.on@000:ke@P.MARK.ke@@P.REDUP.on@%^%[%[%{              Numeral ;  ! eg. "ke- + tujuh-tujuh + -nya" [all the seven of them]
@P.MARK.bare@@P.REDUP.on@000:@P.MARK.bare@@P.REDUP.on@%^%[%[%{              Numeral ;  ! eg. "tujuh-tujuh + -nya" [all the seven of them]
000@P.MARK.ber@:ber@P.MARK.ber@                                             Numeral ;  ! eg. "ber- + tujuh" [in group of seven]
satu%<num%>%<car%>+per%<adv%>+@P.MARK.bare@:seper0000000@P.MARK.bare@       Numeral ;  ! eg. "se- + per + tujuh" [one seventh, 1/7, lit. one each seven]

!! Adjective Derivations
LEXICON DerivingAdjective
@P.MARK.bare@                                                               Adjective ;   ! eg. "cantik" [beautiful]
@P.MARK.bare@@P.REDUP.on@000:@P.MARK.bare@@P.REDUP.on@%^%[%[%{              Adjective ;   ! eg. "cantik-cantik" [beautiful (addressing to plural references)]
00@P.MARK.ke@:ke@P.MARK.ke@                                                 Noun ;        ! eg. "ke- + ibu + -an" [motherly]
00@P.MARK.ke@@P.REDUP.on@000:ke@P.MARK.ke@@P.REDUP.on@%^%[%[%{              Noun ;        ! eg. "ke- + ibu-ibu + -an" [like mothers]
00@P.MARK.ke@:ke@P.MARK.ke@                                                 Adjective ;   ! eg. "ke- + cantik + an" [too beautiful (equally grammatical with "terlalu cantik")]
000@P.MARK.ter@:ter@P.MARK.ter@                                             Adjective ;   ! eg. "ter- + cantik" [the most beautiful (equally grammatical with "paling cantik")]
00@P.MARK.se@@P.FLAG.dernoun@@P.DERADJ.uni@:se@P.MARK.se@@P.FLAG.dernoun@@P.DERADJ.uni@        DerivingNoun-Singular ;      ! eg. "se- + ibu" [one/same mother]
00@P.MARK.se@@P.DERADJ.uni@:se@P.MARK.se@@P.DERADJ.uni@                                        Adjective ;                  ! eg. "se- + cantik" [as beautiful as]
00@P.MARK.se@@P.DERADJ.uni@@P.REDUP.on@000:se@P.MARK.se@@P.DERADJ.uni@@P.REDUP.on@%^%[%[%{     Adjective ;                  ! eg. "se- + besar-besar + -nya" [as big as something can possibly be]
@P.MARK.bare@@P.REDUP.on@000:@P.MARK.bare@@P.REDUP.on@%^%[%[%{                                 Verb ;                       ! eg. "tulis - meN- + tulis" [anything related to writing]

!! Noun Derivations
!! Devided into Singular and Plural just because of the implementation issues
!! The plurality of the Nouns are marked by reduplication
LEXICON DerivingNoun-Singular
@P.NUM.sg@@P.MARK.bare@                                                                         Verb ;        ! eg. "kirim + -an" [package]
@P.NUM.sg@@P.MARK.bare@                                                                         Noun ;        ! eg. "sapu + -an" [stroke (of a sweep/brush, sapu = sweep)]
@P.NUM.sg@@P.MARK.bare@                                                                         Adjective ;   ! eg. "besar + -an" [metric]
@P.NUM.sg@@P.MARK.bare@                                                                         Numeral ;     ! eg. "ratus + -an" [hundreds] xxx?
@P.NUM.sg@@P.MARK.pen@@P.DERNOUN.actor@000:@P.NUM.sg@@P.MARK.pen@@P.DERNOUN.actor@pe%^N         Verb ;        ! eg. "peN- + kirim" [deliverer], "peN- + kirim + -an" [the process of delivering / delivery]
@P.NUM.sg@@P.MARK.pen@@P.DERNOUN.actor@000:@P.NUM.sg@@P.MARK.pen@@P.DERNOUN.actor@pe%^N         Noun ;        ! eg. "peN- + sapu" [sweeper], "peN- + sapu + -an" [the act of stroking]
@P.NUM.sg@@P.MARK.pen@@P.DERNOUN.actor@000:@P.NUM.sg@@P.MARK.pen@@P.DERNOUN.actor@pe%^N         Adjective ;   ! eg. "peN- + dingin" [cooler], "peN- + dingin + -an" [the process of cooling]
@P.NUM.sg@@P.MARK.pen@@P.DERNOUN.actor@000:@P.NUM.sg@@P.MARK.pen@@P.DERNOUN.actor@pe%^N         Numeral ;     ! eg. xxx?
@P.NUM.sg@00@P.MARK.ke@:@P.NUM.sg@ke@P.MARK.ke@                                                 Adjective ;   ! eg. "ke- + dingin + -an" [coolness]
@P.NUM.sg@00@P.MARK.ke@:@P.NUM.sg@ke@P.MARK.ke@                                                 Verb ;        ! eg. xxx?
@P.NUM.sg@00@P.MARK.ke@:@P.NUM.sg@ke@P.MARK.ke@                                                 Noun ;        ! eg. "ke- + raja + -an" [kingdom]
@P.NUM.sg@000@P.MARK.per@:@P.NUM.sg@per@P.MARK.per@                                             Noun ;        ! eg. "per- + toko + -an" [shoping centre]
@P.NUM.sg@000@P.MARK.per@:@P.NUM.sg@per@P.MARK.per@                                             Adjective ;   ! eg. "per- + cepat + -an" [velocity]
@P.NUM.sg@000@P.MARK.per@:@P.NUM.sg@per@P.MARK.per@                                             Numeral ;     ! eg. "per- + tiga + -an" [three-way road junction]
@P.NUM.sg@000@P.MARK.per@:@P.NUM.sg@per@P.MARK.per@                                             Verb ;        ! eg. "per- + main + -an" [game]

!! Examples see above and reduplicate them
LEXICON DerivingNoun-Plural
@P.MARK.bare@@P.REDUP.on@000:@P.MARK.bare@@P.REDUP.on@%^%[%[%{                                                Verb ;
@P.MARK.bare@@P.REDUP.on@000:@P.MARK.bare@@P.REDUP.on@%^%[%[%{                                                Noun ;
@P.MARK.bare@@P.REDUP.on@000:@P.MARK.bare@@P.REDUP.on@%^%[%[%{                                                Adjective ;
@P.MARK.bare@@P.REDUP.on@000:@P.MARK.bare@@P.REDUP.on@%^%[%[%{                                                Numeral ;
@P.MARK.pen@@P.DERNOUN.actor@@P.REDUP.on@000000:@P.MARK.pen@@P.DERNOUN.actor@@P.REDUP.on@%^%[%[%{pe%^N        Verb ;
@P.MARK.pen@@P.DERNOUN.actor@@P.REDUP.on@000000:@P.MARK.pen@@P.DERNOUN.actor@@P.REDUP.on@%^%[%[%{pe%^N        Noun ;
@P.MARK.pen@@P.DERNOUN.actor@@P.REDUP.on@000000:@P.MARK.pen@@P.DERNOUN.actor@@P.REDUP.on@%^%[%[%{pe%^N        Adjective ;
@P.MARK.pen@@P.DERNOUN.actor@@P.REDUP.on@000000:@P.MARK.pen@@P.DERNOUN.actor@@P.REDUP.on@%^%[%[%{pe%^N        Numeral ;
@P.REDUP.on@@P.MARK.ke@00000:@P.REDUP.on@@P.MARK.ke@%^%[%[%{ke                                                Adjective ;
@P.REDUP.on@@P.MARK.ke@00000:@P.REDUP.on@@P.MARK.ke@%^%[%[%{ke                                                Verb ;
@P.REDUP.on@@P.MARK.ke@00000:@P.REDUP.on@@P.MARK.ke@%^%[%[%{ke                                                Noun ;
@P.REDUP.on@@P.MARK.per@000000:@P.REDUP.on@@P.MARK.per@%^%[%[%{per                                            Noun ;
@P.REDUP.on@@P.MARK.per@000000:@P.REDUP.on@@P.MARK.per@%^%[%[%{per                                            Adjective ;
@P.REDUP.on@@P.MARK.per@000000:@P.REDUP.on@@P.MARK.per@%^%[%[%{per                                            Numeral ;
@P.REDUP.on@@P.MARK.per@000000:@P.REDUP.on@@P.MARK.per@%^%[%[%{per                                            Verb ;

