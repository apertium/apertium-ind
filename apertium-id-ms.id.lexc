LEXICON check_aspect
%<perf%>@R.ASPECT.perf@:0@R.ASPECT.perf@                      check_verbvariance ;
%<imp%>@R.ASPECT.imperf@:0@R.ASPECT.imperf@                   check_verbvariance ;
