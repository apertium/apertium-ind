!! -- Section: TAGGING
!! but aslo includes pronoun proclitic and particle

LEXICON tagging
%<vblex%>@R.POS.v@:0@R.POS.v@                                 check_voice ;
%<n%>@R.POS.n@:0@R.POS.n@                                     check_voice ;
%<adj%>@R.POS.adj@:0@R.POS.adj@                               check_voice ;
%<adv%>@R.POS.adv@:0@R.POS.adv@                               check_voice ;
%<num%>@R.POS.num@:0@R.POS.num@                               check_voice ;
%<prn%>@R.POS.prn@:0@R.POS.prn@                               check_voice ;
%<cnjsub%>@R.POS.cnjsub@:0@R.POS.cnjsub@                      check_voice ;
%<cnjcoo%>@R.POS.cnjcoo@:0@R.POS.cnjcoo@                      check_voice ;
%<det%>0@R.POS.det@:0@R.POS.det@                              check_voice ;
%<mod%>0@R.POS.mod@:0@R.POS.mod@                              check_voice ;
%<ij%>0@R.POS.interjection@:0@R.POS.interjection@             check_voice ;
%<qst%>0@R.POS.question@:0@R.POS.question@                    check_voice ;
%<part%>0@R.POS.part@:0@R.POS.part@                           check_voice ;
%<pr%>0@R.POS.pr@:0@R.POS.pr@                                 check_voice ;

LEXICON check_voice
%<actv%>@R.VOICE.av@:0@R.VOICE.av@                            check_aspect ;
%<pasv%>@R.VOICE.pv@:0@R.VOICE.pv@                            check_aspect ;
