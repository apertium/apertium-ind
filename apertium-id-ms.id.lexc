@R.FLAG.dernoun@            na_suffix ;
@R.FLAG.deradj@             adja_suffix ;
@R.FLAG.verbinfl@           va_suffix ;
@D.FLAG@                    tagging ;

LEXICON na_suffix
@R.MARK.bare@@R.POS.n@@P.DERNOUN.bare@                                                   f ;
00@R.MARK.bare@@P.DERNOUN.ent@:an@R.MARK.bare@@P.DERNOUN.ent@                            f ;
00@R.MARK.pen@@P.DERNOUN.act@:an@R.MARK.pen@@P.DERNOUN.act@                              f ;
@R.MARK.pen@                                                                             f ;
00@R.MARK.ke@@P.DERNOUN.abstract@:an@R.MARK.ke@@P.DERNOUN.abstract@                      f ;
00@R.MARK.per@@P.DERNOUN.theme@:an@R.MARK.per@@P.DERNOUN.theme@                          f ;


LEXICON numa_suffix
@D.REDUP@                                                                                                     f ;
000@D.REDUP@@R.DERNUM.ord@@P.DERNUM.ref@:nya@D.REDUP@@R.DERNUM.ord@@P.DERNUM.ref@                             f ;
<[0 .x. "}" "%^REHYPH" "]" "^" 2 "^]"  ] "@R.REDUP.on@" "@C.REDUP@" "@P.DERNUM.ref@" 0:n 0:y 0:a>             f ;
belas@D.REDUP@                                                                                                f ;
<b e l a s [0 .x. "}" "%^REHYPH" "]" "^" 2 "^]"  ] "@R.REDUP.on@" "@C.REDUP@" "@P.DERNUM.ref@" 0:n 0:y 0:a>   f ;

LEXICON adja_suffix
@D.REDUP@@R.DERADJ.positive@@R.POS.adj@                                                                                    f ;
<[0 .x. "}" "%^REHYPH" "]" "^" 2 "^]"  ] "@R.REDUP.on@" "@C.REDUP@" "@P.NUM.pl@" "@R.POS.adj@" "@R.DERADJ.positive@">      f ;
<[0 .x. "}" "%^REHYPH m e %^N" "]" "^" 2 "^]"  ] "@R.REDUP.on@" "@C.REDUP@" "@R.POS.v@" "@R.DERADJ.positive@">             f ;
@D.REDUP@@R.DERADJ.sup@                                                                                                    f ;
<"@D.REDUP@" ["@R.DERADJ.exceed@" | "@R.DERADJ.manner@" | "@R.DERADJ.mimic@"] 0:a 0:n>                                     f ;
<[0 .x. "}" "%^REHYPH" "]" "^" 2 "^]"  ] "@R.REDUP.on@" "@C.REDUP@" "@P.NUM.pl@" "@R.DERADJ.manner@" 0:a 0:n>              f ;
<[0 .x. "}" "%^REHYPH" "]" "^" 2 "^]"  ] "@R.REDUP.on@" "@C.REDUP@" "@R.DERADJ.mimic@" 0:a 0:n>                            f ;
@D.REDUP@@R.DERADJ.possib@000:@D.REDUP@@R.DERADJ.possib@nya                                                                f ;
<[0 .x. "}" "%^REHYPH" "]" "^" 2 "^]"  ] "@R.REDUP.on@" "@C.REDUP@" "@R.DERADJ.possib@" 0:n 0:y 0:a>                       f ;
@D.REDUP@@R.DERADJ.uni@                                                                                                    f ;


